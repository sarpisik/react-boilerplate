{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A custom react-starter-pack for my projects",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "mocha --require @babel/register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles 'src/**/*.test.js'",
    "test:unit:watch": "npm run test:unit -- --watch",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "start": "webpack-dev-server --open --mode development --config webpack.config.js",
    "build": "webpack --mode=production --config webpack.config.js",
    "server": "http-server dist",
    "demo": "npm run build && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarpisik/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "starter",
    "pack",
    "boilerplate",
    "sarp",
    "isik"
  ],
  "author": "Sarp IÅžIK <contact@sarpisik.com> (sarpisik.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sarpisik/react-boilerplate/issues"
  },
  "homepage": "https://github.com/sarpisik/react-boilerplate#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^2.0.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-partials-plugin": "^0.5.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "ignore-styles": "^5.0.1",
    "jsdom": "^14.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.11",
    "sinon": "^7.3.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
